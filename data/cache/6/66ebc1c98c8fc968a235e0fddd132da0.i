gdalwarp -dstnodata -9999 -t_srs EPSG:3006 -cutline ~/Documents/GISdata/Sweden_outline/Sweden_WGS84.shp \
  -crop_to_cutline berggrund/bedrock1km.tif berggrund/bedrock1km_cropped.tif

